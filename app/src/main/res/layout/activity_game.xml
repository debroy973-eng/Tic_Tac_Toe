<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    tools:context=".GameActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <ImageView
                android:id="@+id/array_11"
                android:layout_width="@dimen/layout_width"
                android:layout_height="@dimen/layout_height"
                android:layout_marginLeft="@dimen/imageview_bottom_margin"
                android:layout_marginRight="@dimen/imageview_bottom_margin"
                android:layout_marginTop="@dimen/imageview_bottom_margin"
                android:background="@color/image_background"/>
            <ImageView
                android:id="@+id/array_12"
                android:layout_width="@dimen/layout_width"
                android:layout_height="@dimen/layout_height"
                android:layout_marginRight="@dimen/imageview_bottom_margin"
                android:layout_marginTop="@dimen/imageview_bottom_margin"
                android:background="@color/image_background"/>
            <ImageView
                android:id="@+id/array_13"
                android:layout_width="@dimen/layout_width"
                android:layout_height="@dimen/layout_height"
                android:layout_marginTop="@dimen/imageview_bottom_margin"
                android:background="@color/image_background"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <ImageView
                android:id="@+id/array_21"
                android:layout_width="@dimen/layout_width"
                android:layout_height="@dimen/layout_height"
                android:layout_marginTop="@dimen/imageview_bottom_margin"
                android:background="@color/image_background"
                android:layout_marginLeft="@dimen/imageview_bottom_margin"/>
            <ImageView
                android:id="@+id/array_22"
                android:layout_width="@dimen/layout_width"
                android:layout_height="@dimen/layout_height"
                android:layout_marginTop="@dimen/imageview_bottom_margin"
                android:background="@color/image_background"
                android:layout_marginLeft="@dimen/imageview_bottom_margin"/>
            <ImageView
                android:id="@+id/array_23"
                android:layout_width="@dimen/layout_width"
                android:layout_height="@dimen/layout_height"
                android:layout_marginTop="@dimen/imageview_bottom_margin"
                android:background="@color/image_background"
                android:layout_marginLeft="@dimen/imageview_bottom_margin"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <ImageView
                android:id="@+id/array_31"
                android:layout_width="@dimen/layout_width"
                android:layout_height="@dimen/layout_height"
                android:layout_marginTop="@dimen/imageview_bottom_margin"
                android:background="@color/image_background"
                android:layout_marginLeft="@dimen/imageview_bottom_margin"/>
            <ImageView
                android:id="@+id/array_32"
                android:layout_width="@dimen/layout_width"
                android:layout_height="@dimen/layout_height"
                android:layout_marginTop="@dimen/imageview_bottom_margin"
                android:background="@color/image_background"
                android:layout_marginLeft="@dimen/imageview_bottom_margin"/>
            <ImageView
                android:id="@+id/array_33"
                android:layout_width="@dimen/layout_width"
                android:layout_height="@dimen/layout_height"
                android:layout_marginTop="@dimen/imageview_bottom_margin"
                android:background="@color/image_background"
                android:layout_marginLeft="@dimen/imageview_bottom_margin"/>
        </LinearLayout>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>